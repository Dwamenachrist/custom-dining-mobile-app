{
  "expo": {
    "name": "custom-dining-app",
    "slug": "custom-dining-app",
    "version": "1.0.0",
    "scheme": "custom-dining-app",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-splash-screen",
      [
        "expo-dev-launcher",
        {
          "launchMode": "most-recent"
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#F5F5F0"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kwasiagyapong.customdiningapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "splash": {
        "image": "./assets/icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#F5F5F0"
      },
      "package": "com.kwasiagyapong.customdiningapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#F5F5F0"
      },
      "splash": {
        "image": "./assets/icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#F5F5F0"
      },
      "package": "com.kwasiagyapong.customdiningapp"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "50a0e880-92ca-4a35-92c1-b6b456a15826"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "owner": "dwamenachrist"
  }
}
